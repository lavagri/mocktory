{
  "name": "mocktory",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "pnpm clean && cross-env NODE_ENV=production tsup",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint",
    "release": "release publish"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "http-status-codes": "2.3.0",
    "ioredis": "5.4.1",
    "msw": "2.3.4",
    "strict-event-emitter": "0.5.1",
    "swagger-ui-express": "5.0.1",
    "yaml": "2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@ossjs/release": "0.8.1",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.1.4",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "tsup": "8.2.3",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "npm run lint"
    ]
  },
  "files": [
    "lib",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
